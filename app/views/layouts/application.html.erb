<!DOCTYPE html>
<html>
<head>
  <title>Avellano</title>
  <%= stylesheet_link_tag    'application', media: 'all', '' => true %>
  <%= javascript_include_tag 'application', '' => true %>
  <%= csrf_meta_tags %>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

</head>


<%= render 'layouts/header' unless @disable_nav %>

<body>
<div class="container" >
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>"><%= value %></div>
  <% end %>
  <%= yield %>
</div>
</body>

<%= render 'layouts/footer' %>

</html>
